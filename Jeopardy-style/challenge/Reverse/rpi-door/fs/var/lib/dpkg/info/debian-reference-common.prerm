#! /bin/sh
# prerm script for debian-reference-common
#
# see: dh_installdeb(1)

set -e

case "$1" in
    remove|purge)
	# remove all documents, index and directory
	# rm -Rf /usr/share/doc/Debian/reference || true
	rm -f /usr/share/doc/debian-reference-common/html/index.html
    ;;

    *)
	: # do nothing
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.



exit 0

